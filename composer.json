{
	"name": "tkufd/core",
	"description": "Tamkang University Finance Database for 2024-1 (113-1) course \"Introduction to Database\"",
	"type": "tkufd-core",
	"keywords": [
		"database",
		"finance",
		"tamkang",
		"tku"
	],
	"homepage": "https://github.com/tkufd/tkufd",
	"license": "CC0-1.0",
	"authors": [
		{
			"name": "Winston Sung"
		}
	],
	"support": {
		"issues": "https://github.com/tkufd/tkufd/issues"
	},
	"require": {
		"composer": ">=2.2.24",
		"php": ">=5.3.3"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "3.11.0"
	},
	"autoload": {
		"psr-4": {
			"Tkufd\\": "src"
		}
	},
	"prefer-stable": true,
	"config": {
		"optimize-autoloader": true,
		"prepend-autoloader": false,
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"scripts": {
		"fix": "@phpcbf",
		"test": [
			"@composer validate --no-interaction",
			"@phpcs ."
		],
		"phpcbf": "phpcbf",
		"phpcs": "phpcs -p -s --cache"
	}
}
